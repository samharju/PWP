{
  "openapi": "3.0.2",
  "info": {
    "title": "Lines API",
    "version": "1.0",
    "description": "API for playing the finnish game \"Jatkanshakki\", translates roughly as \"Lumberjacks tic-tac-toe\". Exactly the same as traditional version, but board and number of own markers in line required to win can be changed in range from 3 to 10."
  },
  "servers": [
    {
      "url": "http://localhost:8000/",
      "description": "local"
    }
  ],
  "security": [
    {
      "apiKey": [
        "Authentication"
      ]
    }
  ],
  "tags": [
    {
      "name": "Entrypoint"
    },
    {
      "name": "Leaderboard"
    },
    {
      "name": "Create user"
    },
    {
      "name": "Users"
    },
    {
      "name": "Games"
    },
    {
      "name": "Game Item"
    },
    {
      "name": "Rules"
    },
    {
      "name": "Rule Item"
    }
  ],
  "paths": {
    "/api/": {
      "get": {
        "operationId": "listEntryPoints",
        "description": "Get available resources as controls",
        "security": [
          {}
        ],
        "parameters": [],
        "responses": {
          "200": {
            "content": {
              "application/vnd.mason+json": {
                "example": {
                  "@controls": {
                    "create-user": {
                      "description": "Create new user",
                      "href": "http://localhost:8000/api/users/",
                      "method": "POST",
                      "schema": {
                        "type": "object",
                        "properties": {
                          "username": {
                            "type": "string"
                          },
                          "password": {
                            "type": "string"
                          }
                        }
                      }
                    },
                    "auth-token": {
                      "description": "Get token for user",
                      "href": "http://localhost:8000/api/users/token",
                      "method": "POST",
                      "schema": {
                        "username": {
                          "type": "string"
                        },
                        "password": {
                          "type": "string"
                        }
                      }
                    }
                  }
                }
              }
            },
            "description": "Returns separate available resources by authentication status"
          }
        },
        "tags": [
          "Entrypoint"
        ]
      }
    },
    "/api/users/": {
      "get": {
        "operationId": "listUsers",
        "description": "Get all registered users",
        "parameters": [],
        "responses": {
          "200": {
            "content": {
              "application/vnd.mason+json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/UserCollection"
                  }
                },
                "example": {
                  "items": [
                    {
                      "username": "sami",
                      "win_percentage": "1.00",
                      "wins": 1,
                      "losses": 0,
                      "@controls": {
                        "self": {
                          "description": "Current User",
                          "href": "http://localhost:8000/api/users/sami/"
                        }
                      }
                    },
                    {
                      "username": "esa",
                      "win_percentage": "0.00",
                      "wins": 0,
                      "losses": 1,
                      "@controls": {
                        "self": {
                          "description": "Current User",
                          "href": "http://localhost:8000/api/users/esa/"
                        }
                      }
                    },
                    {
                      "username": "admin",
                      "win_percentage": "0.00",
                      "wins": 0,
                      "losses": 0,
                      "@controls": {
                        "self": {
                          "description": "Current User",
                          "href": "http://localhost:8000/api/users/admin/"
                        }
                      }
                    }
                  ],
                  "@controls": {
                    "up": {
                      "description": "Main menu",
                      "href": "http://localhost:8000/api/"
                    },
                    "create": {
                      "description": "Create new user",
                      "href": "http://localhost:8000/api/users/",
                      "method": "POST",
                      "schema": {
                        "type": "object",
                        "properties": {
                          "password": {
                            "type": "string"
                          }
                        },
                        "required": [
                          "password"
                        ]
                      }
                    }
                  }
                }
              }
            },
            "description": "All registered users."
          },
          "401": {
            "description": "Unauthenticated user",
            "content": {
              "application/vnd.mason+json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                },
                "example": {
                  "@error": {
                    "@message": "Authentication credentials were not provided."
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Users"
        ]
      },
      "post": {
        "operationId": "createUser",
        "description": "Create a new user with provided username and password.",
        "parameters": [],
        "security": [
          {}
        ],
        "requestBody": {
          "content": {
            "application/vnd.mason+json": {
              "schema": {
                "$ref": "#/components/schemas/UserItem"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "User created successfully",
            "headers": {
              "Location": {
                "schema": {
                  "type": "string"
                },
                "description": "Location for created item"
              }
            }
          },
          "400": {
            "description": "Error message",
            "content": {
              "application/vnd.mason+json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                },
                "example": {
                  "@error": {
                    "@message": "username: A user with that username already exists."
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Create user"
        ]
      }
    },
    "/api/users/{username}/": {
      "get": {
        "operationId": "retrieveUser",
        "description": "Get user details.",
        "parameters": [
          {
            "name": "username",
            "in": "path",
            "required": true,
            "description": "A unique value identifying this user.",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "User details",
            "content": {
              "application/vnd.mason+json": {
                "schema": {
                  "$ref": "#/components/schemas/UserItem"
                },
                "example": {
                  "username": "teppo",
                  "win_percentage": "0.66",
                  "wins": 2,
                  "losses": 1,
                  "@controls": {
                    "self": {
                      "description": "Current user",
                      "href": "http://localhost:8000/api/users/teppo/"
                    },
                    "collection": {
                      "description": "List of Users",
                      "href": "http://localhost:8000/api/users/"
                    },
                    "history": {
                      "description": "This users game history",
                      "href": "http://localhost:8000/api/games/?history=teppo"
                    },
                    "rules-created": {
                      "description": "Rules created by user",
                      "href": "http://localhost:8000/api/rules/?author=teppo"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthenticated user",
            "content": {
              "application/vnd.mason+json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                },
                "example": {
                  "@error": {
                    "@message": "Authentication credentials were not provided."
                  }
                }
              }
            }
          }
        },
        "tags": [
          "User Item"
        ]
      },
      "put": {
        "operationId": "updateUser",
        "description": "Change user password",
        "parameters": [
          {
            "name": "username",
            "in": "path",
            "required": true,
            "description": "A unique value identifying this user.",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserUpdate"
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "Password updated successfully."
          },
          "401": {
            "description": "Unauthenticated user",
            "content": {
              "application/vnd.mason+json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                },
                "example": {
                  "@error": {
                    "@message": "Authentication credentials were not provided."
                  }
                }
              }
            }
          }
        },
        "tags": [
          "User Item"
        ]
      },
      "delete": {
        "operationId": "destroyUser",
        "description": "Remove user. Only allowed by user themself.",
        "parameters": [
          {
            "name": "username",
            "in": "path",
            "required": true,
            "description": "A unique value identifying this user.",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "User removed successfully."
          },
          "401": {
            "description": "Unauthenticated user",
            "content": {
              "application/vnd.mason+json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                },
                "example": {
                  "@error": {
                    "@message": "Authentication credentials were not provided."
                  }
                }
              }
            }
          },
          "403": {
            "description": "No object permissions.",
            "content": {
              "application/vnd.mason+json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                },
                "example": {
                  "@error": {
                    "@message": "You do not have permission to perform this action."
                  }
                }
              }
            }
          }
        },
        "tags": [
          "User Item"
        ]
      }
