            "description": "Faulty request, username or password incorrect",
            "content": {
              "application/vnd.mason+json": {
                "example": {
                  "@error": {
                    "@message": "non_field_errors: Unable to log in with provided credentials."
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Users"
        ]
      }
    },
    "/api/games/{id}/add-move/": {
      "put": {
        "operationId": "addMoveGame",
        "description": "Make your mark in the gameboard. Only allowed for user whose turn it is.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "A unique integer value identifying this game.",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AddMove"
              },
              "example": {
                "row": 2,
                "column": 2
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "Game updated successfully."
          },
          "400": {
            "description": "Selected move was illegal, game is not ongoing or its other players turn.",
            "content": {
              "application/mason.vnd+json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                },
                "example": {
                  "@error": {
                    "@message": "Select empty slot on board"
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthenticated user",
            "content": {
              "application/vnd.mason+json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                },
                "example": {
                  "@error": {
                    "@message": "Authentication credentials were not provided."
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Game Item"
        ]
      }
    },
    "/api/games/{id}/join/": {
      "put": {
        "operationId": "joinGame",
        "description": "Join a game with free slot.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "A unique integer value identifying this game.",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/vnd.mason+json": {
                "example": {
                  "detail": "Successfully joined game",
                  "@controls": {
                    "self": {
                      "description": "Current game",
                      "href": "http://localhost:8000/api/games/1/"
                    }
                  }
                }
              }
            },
            "description": "User was updated as the second player."
          },
          "401": {
            "description": "Unauthenticated user",
            "content": {
              "application/vnd.mason+json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                },
                "example": {
                  "@error": {
                    "@message": "Authentication credentials were not provided."
                  }
                }
              }
            }
          }
        },
        "tags": [
          "Game Item"
        ]
      }
    }
  },
